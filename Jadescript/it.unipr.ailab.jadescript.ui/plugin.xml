		</page>
		<page
			category="it.unipr.ailab.jadescript.Jadescript"
			class="it.unipr.ailab.jadescript.ui.JadescriptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
