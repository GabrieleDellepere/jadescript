			class="it.unipr.ailab.jadescript.ui.JadescriptExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="it.unipr.ailab.jadescript.Jadescript"
			name="Jadescript">
			<keywordReference id="it.unipr.ailab.jadescript.ui.keyword_Jadescript"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
